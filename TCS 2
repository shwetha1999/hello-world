#include <iostream>
using namespace std;

void prime(int n)
{
    int c=0,i;
    for( i=1;i<=n;i++)
    {
        if(n%i==0)
            c++;
    }
    if(c==2)
        cout<<"prime number"<<endl;
    else
        cout<<"not prime number"<<endl;

}
int main()
{
 int n;
 cout<<"enter a num"<<endl;
 cin>>n;
 if(n<0)
 {
     cout<<"please emter a positive num"<<endl;
 }
 else
   prime(n);
return 0;
}
