#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,n,sum=0;
  cin>>n;
  int arr[n];

  for(i=0;i<n;i++)
  {
      cin>>arr[i];
  }
  for(i=0;i<n;i++)
  {
      cout<<arr[i];
  }
  cout<<endl;
  for(i=0;i<n-1;i++)
  {
  sum=arr[i+1]+arr[i];
  cout<<sum;
  }

  return 0;
}
