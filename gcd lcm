#include<stdio.h>

int main()
{
    
int n1,n2,i,gcd,lcm;
printf("enter two numbers");
scanf("%d %d",&n1,&n2);
for(i=1;i<=n1;i++)
{
    if(n1%i==0 && n2%i==0)
    gcd=i;
    
}
printf("   gcd is %d",gcd);
lcm=(n1*n2)/gcd;
printf("   lcm is %d",lcm);
return 0;
}



output:
enter two numbers
14 7
gcd is 7 lcm is 14
