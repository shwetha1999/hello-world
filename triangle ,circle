//area of a triangle
#include<stdio.h>
#include<math.h>
int main()
{
	int h,b,area;
	printf("enter  number\n");
	scanf("%d %d",&h,&b);
	area=(h*b)/2;
	printf("area  of trianglre is %d",area);
	return 0;
	
	
}

input:4 5
output:
enter  number
area  of trianglre is 10



//area of a circle
#include<stdio.h>
#include<math.h>
int main()
{
	int r;
	float area;
	printf("enter  number\n");
	scanf("%d",&r);
	area=3.14*r*r;
	printf("area  of circle is %f",area);
	return 0;
	
	
}
input: 3
output:
enter  number
area  of circle is 28.260000


//area of a circle
#include<stdio.h>
#include<math.h>
int main()
{
	int r;
	float area,circum;
	printf("enter  number\n");
	scanf("%d",&r);
	area=3.14*r*r;
	printf("area  of circle is %f\n",area);
	circum=2*3.14*r;
	printf("circumference of a circle is %f\n",circum);
	
	return 0;
	
	
}

input:3
output:
enter  number
area  of circle is 28.260000
circumference of a circle is 18.840000





printing without using pf and sf


#include <stdio.h>

int main(void)
{

	char d;
	while((d=getchar())!=EOF)
	putchar(d);
	
	return 0;
}
input:hello world

output:hello world
