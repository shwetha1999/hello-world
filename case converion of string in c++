#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s1("Shwetha H");
    cout<< s1 << endl;
    transform(s1.begin(),s1.end(),s1.begin(),::toupper);
    cout<<s1 << endl;
    transform(s1.begin(),s1.end(),s1.begin(),::tolower);
    cout<<s1 << endl;
}

output:
Shwetha H
SHWETHA H
shwetha h
