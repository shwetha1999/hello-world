#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
  int arr[]={1,1,2,3};
  int n=sizeof(arr)/sizeof(arr[0]);
  for(int i=0;i<n;i++)
  cout<< arr[i] <<" ";

}
 output:
 1 1 2 3




#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
  array<int,6>arr;
  arr={1,2,3,4,5,6};
  int n=sizeof(arr)/sizeof(arr[0]);
  for(int i=0;i<n;i++)
  {

      cout<< arr.at(i)<<" ";

  }
  cout<< endl;
  return 0;
}



#include<iostream>
#include<array> // for front() and back()
using namespace std;
int main()
{
    array<int,6> ar = {1, 2, 3, 4, 5, 6};
    cout << "First element of array is : ";
    cout << ar.front() << endl;
    cout << "Last element of array is : ";
    cout << ar.back() << endl;

    return 0;
    
    
    
    
    
    #include<iostream>
#include<array> 
using namespace std;
int main()
{
    array<int,6> ar = {1, 2, 3, 4, 5, 6};

    cout << ar.size() << endl;
    cout<< sizeof(ar)<<endl;
    cout << ar.max_size() << endl;

    return 0;

}
output:
