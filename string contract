#include <stdio.h>
int main()
{
   char str1[50], str2[50], i, j;
   printf("\nEnter first string: ");
   gets(str1);
   printf("\nEnter second string: ");
   gets(str2);
   i=strlen(str1);
   for(j=0; str2[j]!='\0'; j++, i++)
   {
      str1[i]=str2[j];
   }

   str1[i]='\0';
   printf("\nOutput: %s",str1);

   return 0;
}







#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
   char str1[50], str2[50], i, j;
   printf("\nEnter first string: ");
   gets(str1);
   printf("\nEnter second string: ");
   gets(str2);
   strcat(str1,str2);
   printf("%s",str1);
   return 0;
   
   
   
   
   #include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    char str1[50], str2[50],str3[50] ,i, j;
   printf("\nEnter first string: ");
   gets(str1);
   printf("\nEnter second string: ");
   gets(str2);
   printf("\nEnter second string: ");
   gets(str3);
   strcat(str1,str2);
   strcat(str1,str3);
   printf("%s",str1);
   return 0;
}

}



