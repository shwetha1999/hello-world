#include<iostream>
using namespace std;
int main()
{
	int t[10][10];
	int a,b,i,j;
	cout<<"Enter 1st number from 0 to 9:";
	cin>>a;
	cout<<"Enter 2nd number from 0 to 9:";
	cin>>b;
	for(int i=1;i<=a;i++)
	{
		for(int j=1;j<=b;j++)
		{
			t[i][j]=i*j;
			cout<<i<<"*"<<j<<"="<<t[i][j]<<endl;

		}

	}

}
