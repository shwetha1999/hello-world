#include<stdio.h>
#include<math.h>
int main()
{
	int a,b;
	printf("enter two numbers\n");
	scanf("%d %d",&a,&b);
	printf("%d\n",a<b);
	printf("%d\n",a<=b);
	printf("%d\n",a>b);
	printf("%d\n",a>=b);
	printf("%d\n",a==b);
	printf("%d\n",a!=b);
	return 0;
}


