#include <stdio.h>

int main()
{
   int n,i,count=0;
   printf("enter a number\n");
   scanf("%d",&n);
   for(i=1;i<=n;i++)
   
    {   if(n%i==0)
       count++;
   }
   if(count==2)
   {
       printf("\n prime num");
   }
   else
   printf("\n not a prime");
   return 0;
}


output:
enter a number
12
not a prime



#include <stdio.h>

int main()
{
   int n,i,j,count=0;
   printf("enter a number\n");
   scanf("%d",&n);
   for(i=2;i<=n;i++)
   {
count=0;
for(j=1;j<=i;j++)
{
    if(i%j==0)
    count++;
}
if(count==2)
printf("  %d  ",i);
}
    return 0;
}

output:
enter a number
12
1 2 3 5 7 11


