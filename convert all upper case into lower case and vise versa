#include <stdio.h>
#include <string.h>
int main ()
{
char s[40];
int i;
 printf("enter a string\n");
 gets(s);
 puts(s);

 for(i=0;s[i]!='\0';i++)
 {
    if(s[i]>=65 && s[i]<=90)
      s[i]=s[i]+32;

    else
     if(s[i]>=97 && s[i]<=122)
      s[i]=s[i]-32;
 }
puts(s);

return 0;
}





#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i;
    string s1("Shwetha H");
    cout<< s1 << endl;
    for( i=0;i<s1.length();i++)
    {
    if(s1[i]>=97 &&s1[i]<=122)
      {
      s1[i]=s1[i]-32;
      cout<<s1[i];
      }

else
    cout<<s1[i];

   }

   return 0;
}
  
  
  
  output:
  Shwetha H
  SHWETHA H
  
  
  
