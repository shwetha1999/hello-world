#include <iostream>
#include <algorithm>

using namespace std;

int main()
{
    int a[5]={ 1, 2, 3, 4 ,5};
    int key=10;
    if(binary_search(a, a+5, key))
    cout<< " found "<<endl;
    else
    cout<< " not found "<<endl;
    

    return 0;
}


