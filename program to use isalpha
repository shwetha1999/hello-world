#include<iostream>
#include<bits/stdc++.h>
#include<ctype.h>
using namespace std;
int main()
{
   int n,i;
   string a;
   cin>>a;
   n=a.length();
   for(i=0;i<n;i++)
   {
      if(isalpha(a[i]))
      {
          if(a[i]=='Z'||a[i]=='z')
            a[i]=a[i]-25;
          else
            a[i]++;

      }
      cout<<a[i];
   }

}
