#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i;
    string s1("Shwetha H");
    cout<< s1 << endl;
    for( i=0;i<s1.length();i++)
    {
        if(s1[i]>=65 &&s1[i]<=90)
        {
            s1[i]=s1[i]+32;
            cout<<s1[i];
        }
        else
            if(s1[i]>=97 &&s1[i]<=122)
{
    s1[i]=s1[i]-32;

            cout<<s1[i];
}
    }
     cout<<endl;
   return 0;
}
