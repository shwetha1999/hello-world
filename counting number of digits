// counting the number of digits
#include<stdio.h>
int main (void)
{
	int n,c=0;
	printf("enter the number \n ");
	scanf("%d",&n);
	for(;n>0;n=n/10)
	c++;
	printf("the length is %d",c)
	return 0;
}
input 12345
output the length is 



Alternative


#include<stdio.h>
int main()
{
int n,x;
printf("enter a num\n");
scanf("%d",&n);


x=printf("%d",n);
printf("\n");
printf("%d",x);
return 0;
}
