#include <stdio.h>

int main()
{char fullname[50];
   
    
    printf("enter a string\n");
    gets(fullname);
    printf("the fullname is %s ",fullname);
    
    
    
    return 0;
}
output:
geeks for geeks
the fullname is geeks for geeks



#include <stdio.h>

int main()
{
    char fullname[50];
   
    
    printf("enter a number\n");
    scanf("%s",&fullname);
    printf("the fullname is %s ",fullname);
    
    
    
    return 0;
}

output:
geeks for geeks
the fullname is geeks






#include <stdio.h>
#include<string.h>
int main()
{
    char a[50];
    printf("enter a string\n");
  gets(a);
    printf("the string  is ");
    puts(a);

    
    
    return 0;
}

