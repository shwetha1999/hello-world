#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,n,flag=0;
  cin>>n;
  int arr[n];

  for(i=0;i<n;i++)
  {
      cin>>arr[i];
  }

  sort(arr,arr+n);
  for(i=0;i<n;i++)
  {
      cout<<arr[i];
  }
  for(i=0;i<n-1;i++)
{
      if(arr[i+1]>arr[i]+1)
      {

          flag=1;
          break;
      }
}
if(flag==1)
    cout<<"non concecutive"<<endl;
  else cout<<"concecutive"<<endl;
  return 0;
}



output:
1 2 3 4 5
concecutive
1 2 3 5 9
non concecutive
4 5 8 6 7
concecutive
