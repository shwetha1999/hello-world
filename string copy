#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
int n,i;
  char s1[100]={"shwetha"};
  char s2[100];
  n=strlen(s1);
  printf("%d\n",n);
  for(i=0;i<n;i++)// or ( i=0;s1[i]!='\0';i++)
  {
      s2[i]=s1[i];
  }
  s2[i]='\0';
  printf("%s",s2);
  return 0;
}
